#source /opt/jumpscale7/env.sh;//opt/jumpscale7/bin/
#base of repo where code or files are e.g. http://git.aydo.com/binary/base
service.git.url                = 'https://git.aydo.com/binary/openvcloud/'
service.installed.checksum     = '7d5a1f6a98046dea359e0d377593d0a1'
service.instance               = 'main'

#how many instances can be installed of this package, if 1 the instance name is always 'main'
service.instances.maxnr        = '1'
service.name                   = 'oauthserver'
service.platform.supported     =
    'generic',

service.process.1              =
    args:,
    cmd:'./oauthserver',
    cwd:'/opt/jumpscale7/apps/oauthserver/',
    env:,
    filterstr:'oauthserver',
    ports:'8010',
    prio:'20',
    startupmanager:'tmux',
    timeout_start:'20',
    timeout_stop:'20',
