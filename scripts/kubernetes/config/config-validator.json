                    "macaddress": {
                      "$id": "/properties/nodes/properties/storage/items/properties/management/properties/macaddress",
                      "type": "string",
                      "title": "The Macaddress Schema",
                      "description": "An explanation about the purpose of this instance.",
                      "default": "",
                      "examples": [
                        "2C:60:0C:BC:26:65"
                      ]
                    }
