        "properties": {
          "management": {
            "$id": "/properties/network/properties/management",
            "type": "object",
            "required": ["gateway", "vlan", "network"],
            "properties": {
              "gateway": {
                "$id": "/properties/network/properties/management/properties/gateway",
                "type": "string",
                "title": "The Gateway Schema",
