                  "properties": {
                    "password": {
                      "$id": "/properties/nodes/properties/storage/items/properties/ipmi/properties/password",
                      "type": "string",
                      "title": "The Password Schema",
                      "description": "An explanation about the purpose of this instance.",
                      "default": "",
